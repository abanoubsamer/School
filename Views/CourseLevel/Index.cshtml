@model IEnumerable<School.Models.CourseLevel>



@{
	ViewData["Title"] = "Index";
}
<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table table-bordered  " id="myTable" style="width:100%">
    <thead>
        <tr>
            <td class="text-center">
                @Html.DisplayNameFor(model => model.Level)
            </th>
          
           
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td class="text-center">
                    @Html.DisplayFor(modelItem => item.Level)
                </td>

                <td class="text-center">
                    <a asp-action="Edit" asp-route-id="@item.CourseLevelId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.CourseLevelId">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.CourseLevelId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>



